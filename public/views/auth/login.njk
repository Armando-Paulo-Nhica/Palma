<body>
	
	<div class="limiter">
		<div class="container-login100">
			<div class="wrap-login100">
				<form class="login100-form validate-form ">
					<span class="login100-form-title">
						<img src="../../images/logo3.png" alt="logo" width="85px">
					</span>
					<span class="login100-form-title mb-80">
						Sistema de Gestão
					</span> 

					<div class="wrap-input100 validate-input" data-validate = "Nome do utilizador inválido">
						<input class="input100" id="username" type="text" name="text">
						<span class="focus-input100" data-placeholder="Nome do utilizador"></span>
					</div>

					<div class="wrap-input100 validate-input" data-validate="Palavra-passe inválida">
						<span class="btn-show-pass">
							<i class="zmdi zmdi-eye"></i>
						</span>
						<input class="input100" id="password" type="password" name="pass">
						<span class="focus-input100" data-placeholder="Palavra-passe"></span>
					</div>

					<div class="container-login100-form-btn">
						<div class="wrap-login100-form-btn">
							<div class="login100-form-bgbtn"></div>
							<button class="login100-form-btn" id="login-btn">
								Entrar
							</button>
						</div>
					</div> 

					<div class="text-center p-t-50">
						<span class="txt1">
							Esqueceu sua senha?
						</span>

						<a class="txt2" href="/user/reset/pw">
							Recuperar senha
						</a>
					</div>
				</form>
			</div>
